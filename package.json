{
  "name": "teselagen-react-components",
  "version": "30.13.18",
  "description": "Teselagen React Component Library",
  "main": "lib/index.js",
  "types": "./index.d.ts",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "index.d.ts"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files --no-demo",
    "buildWithDemo": "nwb build-react-component --copy-files",
    "//build-watch": "nodemon --watch src --exec \"rm .babelrc; pnpm build\" ",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo --port 3333",
    "analyze-build": "ANALYZE_BUNDLE=1 pnpm buildWithDemo && webpack-bundle-analyzer demo/dist/stats.json build",
    "c": "pnpm cypress open --e2e --browser chrome",
    "test": "pnpm cypress run",
    "changelog": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "@blueprintjs/core": "3.52.0",
    "@blueprintjs/datetime": "3.23.19",
    "@blueprintjs/select": "3.18.11",
    "@teselagen/react-list": "^0.8.18",
    "@teselagen/react-table": "^6.10.11",
    "axios": "^0.21.1",
    "browser-or-node": "^2.0.0",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "dayjs": "^1.10.4",
    "dom-scroll-into-view": "^2.0.1",
    "downloadjs": "^1.4.7",
    "fuse.js": "^6.6.2",
    "fuzzysearch": "^1.0.3",
    "immer": "^9.0.15",
    "is-mobile": "^3.0.0",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "math-expression-evaluator": "^1.3.7",
    "nanoid": "^4.0.0",
    "papaparse": "^5.3.2",
    "prop-types": "^15.6.2",
    "qs": "^6.9.6",
    "react-color": "^2.19.3",
    "react-dropzone": "^11.4.2",
    "react-markdown": "4.0.6",
    "react-rnd": "^10.2.4",
    "react-router-dom": "^4.3.1",
    "react-sortable-hoc": "^0.6.8",
    "react-transition-group": "^2.4.0",
    "recompose": "npm:react-recompose@0.31.1",
    "redux-form": "^8.3.8",
    "tg-client-query-builder": "^2.14.2",
    "tippy.js": "^6.3.7",
    "url-join": "^4.0.1",
    "use-deep-compare-effect": "^1.6.1"
  },
  "devDependencies": {
    "@types/lodash": "npm:@types/lodash-es",
    "auto-changelog": "^2.2.1",
    "chance": "^1.1.7",
    "cypress": "11.2.0",
    "eslint-config-teselagen": "^6.0.16",
    "gh-pages": "^2.2.0",
    "husky": "^1.1.3",
    "lint-staged": "^9.2.3",
    "nwb": "^0.24.5",
    "prettier": "^1.16.4",
    "raw-loader": "^0.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.5",
    "redux": "^4.1.2",
    "redux-thunk": "2.4.1",
    "stats-webpack-plugin": "^0.7.0",
    "vite": "^4.0.1",
    "webpack-bundle-analyzer": "^3.7.0"
  },
  "resolutions": {
    "listr": "0.14.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "cypress/**/*.js|nwb.config.js": [
      "eslint --rule 'react/jsx-curly-brace-presence: 1' --fix --max-warnings=0",
      "prettier --write",
      "git add"
    ],
    "src/**/*.js": [
      "eslint --rule 'react/jsx-curly-brace-presence: 1' --fix --max-warnings=0",
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "license": "MIT",
  "repository": "https://github.com/TeselaGen/teselagen-react-components",
  "homepage": "https://github.com/TeselaGen/teselagen-react-components",
  "keywords": [
    "react-component",
    "teselagen",
    "blueprintjs",
    "multi-select",
    "side-by-side",
    "menu-bar",
    "timeline",
    "confirmation-dialog",
    "resizable-dialog"
  ]
}
