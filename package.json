{
  "name": "teselagen-react-components",
  "version": "23.0.11",
  "description": "Teselagen React Component Library",
  "main": "lib/index.js",
  "homepage": "http://teselagen.github.io/teselagen-react-components",
  "//module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files --no-demo",
    "buildWithDemo": "nwb build-react-component --copy-files",
    "build-watch": "nodemon --watch src --exec \"rm .babelrc; yarn build\" ",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo --port 3333",
    "precommit": "lint-staged",
    "prepare": "in-publish && npm run build || not-in-publish",
    "postpublish": "yarn deploy",
    "predeploy": "yarn buildWithDemo",
    "deploy": "gh-pages -d demo/dist",
    "fontopen": "make fontopen",
    "fontsave": "make fontsave",
    "test": "jest --env=jsdom --maxWorkers 2",
    "test-coverage": "yarn test -- --coverage",
    "test:staged": "cross-env CI=true yarn test -- --findRelatedTests",
    "test-fix": "git reset . && cross-env CI=true yarn test --updateSnapshot",
    "test-watch": "yarn test -- --watch",
    "test-debug": "node --inspect --debug-brk ./node_modules/.bin/jest src --runInBand "
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.0.8",
    "cross-env": "^5.1.4",
    "deep-equal": "^1.0.1",
    "dom-scroll-into-view": "^1.2.1",
    "file-saver": "^1.3.8",
    "json.sortify": "^2.2.2",
    "lodash": "^4.17.5",
    "lodash.flatmap": "^4.5.0",
    "lru-memoize": "^1.0.2",
    "math-expression-evaluator": "^1.2.17",
    "mime-types": "^2.1.19",
    "moment": "^2.22.2",
    "nodemon": "^1.17.2",
    "papaparse": "^4.5.0",
    "pascal-case": "^2.0.1",
    "paths-js": "^0.4.7",
    "pluralize": "^7.0.0",
    "query-string": "^5.1.0",
    "react-addons-pure-render-mixin": "^15.6.0",
    "react-dropzone": "^4.2.12",
    "react-resize-detector": "^2.3.0",
    "react-rnd": "^8.0.2",
    "react-sortable-hoc": "^0.6.8",
    "react-values": "^0.2.4",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "uniqid": "^4.1.1",
    "ve-range-utils": "^2.5.0",
    "ve-sequence-utils": "^3.2.57"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^3",
    "@blueprintjs/datetime": "^3",
    "apollo-cache-invalidation": "^0.0.3",
    "react-apollo": "^2.0.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.3",
    "react-table": "^6.8.0",
    "redux-form": "^7.3.0",
    "tg-client-query-builder": "^2.6.0"
  },
  "devDependencies": {
    "@blueprintjs/core": "^3.3.0",
    "@blueprintjs/datetime": "^3.1.0",
    "apollo-boost": "^0.1.4",
    "apollo-cache-invalidation": "^0.0.3",
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.4.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chance": "^1.0.13",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-teselagen": "^4.0.12",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "gh-pages": "^1.1.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "in-publish": "^2.0.0",
    "jest": "^23.5.0",
    "lint-staged": "^7.0.0",
    "nwb": "^0.23.0",
    "prettier": "^1.14.2",
    "raw-loader": "^0.5.1",
    "react": "^16.4.1",
    "react-apollo": "^2.1.8",
    "react-dom": "^16.4.1",
    "react-live": "^1.10.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-table": "^6.8.0",
    "react-test-renderer": "^16.4.1",
    "react-transition-group": "^2.2.1",
    "redux-form": "^7.3.0",
    "redux-thunk": "*",
    "sinon": "^4.4.9",
    "tg-client-query-builder": "^2.6.0"
  },
  "lint-staged": {
    "src/**/*.js ": [
      "eslint --fix",
      "prettier --write",
      "yarn test:staged",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|png)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/",
      "/es/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js"
  },
  "license": "MIT",
  "repository": "https://github.com/TeselaGen/teselagen-react-components",
  "keywords": [
    "react-component",
    "teselagen"
  ]
}
